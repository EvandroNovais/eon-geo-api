# Environment
NODE_ENV=development

# Server
PORT=3000
HOST=localhost

# Redis
REDIS_URL=redis://localhost:6379
REDIS_TTL=86400

# API Keys (optional)
OPENCAGE_API_KEY=
GOOGLE_MAPS_API_KEY=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_FILE=logs/app.log

# Cache
CACHE_TTL_SECONDS=86400